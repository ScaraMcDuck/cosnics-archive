<?xml version="1.0" encoding="UTF-8"?>
<block name="personal_calendar_day">
	<settings>
		<category name="time">
			<setting name="hour_step" field="text" default="1" />
			<setting name="time_start" field="text" default="9" />
			<setting name="time_end" field="text" default="17" />
		</category>
		<category name="display">
			<setting name="show_empty" field="radio" default="1">
				<options type="static">
					<option name="true" value="1" />
					<option name="false" value="0" />
				</options>
			</setting>
		</category>
	</settings>
</block>