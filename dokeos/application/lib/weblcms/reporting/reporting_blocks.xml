<?xml version="1.0" encoding="UTF-8"?>
<application name="weblcms">
	<reporting_block name="UserInformation"
		   application="user"
		   function="getUserInformation"
		   displaymode="Table"
		   width="500px"
		   height="240px" />
	<reporting_block name="UserPlatformStatistics"
		   application="user"
		   function="getUserPlatformStatistics"
		   displaymode="Table"
		   width="500px"
		   height="240px" />
	<reporting_block name="CourseInformation"
		   application="weblcms"
		   function="getCourseInformation"
		   displaymode="Text"
		   width="500px"
		   height="500px" />
	<reporting_block name="CourseUserLearningpathInformation"
		   application="weblcms"
		   function="getCourseUserLearningpathInformation"
		   displaymode="Table"
		   width="500px"
		   height="500px" />
	<reporting_block name="CourseUserExerciseInformation"
		   application="weblcms"
		   function="getCourseUserExcerciseInformation"
		   displaymode="Table"
		   width="500px"
		   height="500px" />
    <reporting_block name="WeblcmsNoOfCourses"
           application="weblcms"
           function="getNoOfCourses"
           displaymode="Table"
           width="500px"
           height="500px" />
    <reporting_block name="WeblcmsNoOfObjectsPerType"
           application="weblcms"
           function="getNoOfObjectsPerType"
           displaymode="Table"
           width="500px"
           height="500px" />
</application>